  "testCases": [
    {
      "name": "Get Single User",
      "status": "Not Started",
      "testData": [
        {
          "name": "Fetch User with ID 2",
          "method": "GET",
          "endpoint": "/objects?id=3&id=5&id=10",
          "headers": {
            "Content-Type": "application/json"
          },
          "assertions": [
            {
              "type": "equals",
              "jsonPath": "$.[0].name",
              "expected": "Apple iPhone 12 Pro Max"
            },
            {
              "type": "equals",
              "jsonPath": "$.[1].data.price",
              "expected": 689.99
            }
          ],
          "responseSchema": {
            "$schema": "http://json-schema.org/draft-07/schema#",
            "type": "array",
            "items": {
              "type": "object",
              "required": [
                "id",
                "name",
                "data"
              ],
              "properties": {
                "id": {
                  "type": "string"
                },
                "name": {
                  "type": "string"
                },
                "data": {
                  "type": "object",
                  "properties": {
                    "color": {
                      "type": "string"
                    },
                    "capacity GB": {
                      "type": "number"
                    },
                    "price": {
                      "type": "number"
                    },
                    "Capacity": {
                      "type": "string"
                    },
                    "Screen size": {
                      "type": "number"
                    }
                  },
                  "additionalProperties": false
                }
              },
              "additionalProperties": false
            }
          },
          "store": {
            "userName": "$.[0].name"
          }
        }
      ]
    },
    {
      "name": "List of all objects",
      "status": "Not Started",
      "testData": [
        {
          "name": "Listing All Objects Using the endpoint",
          "method": "GET",
          "endpoint": "/objects",
          "headers": {
            "Content-Type": "application/json"
          },
          "assertions": [
            {
              "type": "equals",
              "jsonPath": "$.[0].name",
              "expected": "Apple iPhone 12 Pro Max"
            },
            {
              "type": "size",
              "jsonPath": "$.",
              "expected": 5
            },
            {
              "type": "statusCode",
              "expected": 200
            }
          ],
          "store": {
            "userName": "$.[0].name"
          }
        }
      ]
    },
    {
      "name": "Add Object",
      "status": "Not Started",
      "testData": [
        {
          "name": "Adding a new Single Object",
          "method": "POST",
          "endpoint": "/objects",
          "headers": {
            "Content-Type": "application/json"
          },
          "preProcess": [
            {
              "var": "randomEmail",
              "function": "faker.email"
            },
            {
              "var": "encryptedPassword",
              "function": "encrypt",
              "args": [
                "P@ss1234"
              ]
            },
            {
              "var": "authToken",
              "function": "authToken"
            }
          ],
          "body": {
            "name": "Apple MacBook Pro 16",
            "data": {
              "year": 2019,
              "price": 1849.99,
              "CPU model": "Intel Core i9",
              "Hard disk size": "1 TB"
            }
          },
          "assertions": [
            {
              "type": "exists",
              "jsonPath": "$.id"
            },
            {
              "type": "equals",
              "jsonPath": "$.name",
              "expected": "Apple MacBook Pro 16"
            },
            {
              "type": "statusCode",
              "expected": 200
            }
          ],
          "store": {
            "newObjectID": "$.id"
          }
        }
      ]
    }
  ],
